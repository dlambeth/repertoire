<div class="col-md-3">
  <div id="mylists" class="list-group">
    <h5>Set Lists</h5>
    {{#link-to 'myLists.new' class="list-group-item"}}+ Add a new list{{/link-to}}
    {{#each}}
      <div class="list-group-item">
      {{#link-to 'myList' this.id }}{{title}}{{/link-to}}
      <a href="#" {{action "deleteList" this}}><i class="fa fa-trash-o fa-fw"></i>Delete</a>
      </div>
    {{/each}}
  </div>
</div>
<div class="col-md-9">
{{outlet}}
</div>